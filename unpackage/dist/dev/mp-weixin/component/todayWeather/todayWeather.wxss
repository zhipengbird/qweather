.l-page-city-weather {
  margin: 3%;
}
.c-city-weather-current {
  border: none;
  border-radius: 16px;
}
@media (max-width: 739px) {
.c-city-weather-current .container {
    max-width: 540rpx;
}
.c-city-weather-current .hidden-740 {
    display: none !important;
}
}
.c-city-weather-current .current-live {
  margin-top: 6px;
  white-space: nowrap;
}
.c-city-weather-current .current-live .current-live__item {
  display: inline-block;
  vertical-align: middle;
  position: relative;
  width: 50%;
  max-width: 50%;
}
.c-city-weather-current .current-live .current-live__item .icon {
  background-color: transparent;
  border-radius: 0.25rem;
  font-size: 3.5rem;
  text-align: right;
}
.c-city-weather-current .current-live .current-live__item .temp {
  font-size: 52rpx;
  line-height: 1;
  padding-left: 10px;
}
.c-city-weather-current .current-live .current-live__item .tempdesc {
  font-size: 30rpx;
  font-weight: 500;
  padding-left: 10px;
}
.c-city-weather-current .current-live .cityInfo {
  display: inline-block;
  font-size: 30rpx;
  top: 10rpx;
  position: absolute;
}
.c-city-weather-current .current-live .air-tag {
  display: inline-block;
  padding: 8rpx 15rpx;
  font-size: 36rpx;
  line-height: 38rpx;
  text-align: center;
  white-space: nowrap;
  border-radius: 30rpx;
  color: white;
  position: absolute;
  top: -15rpx;
  right: -20rpx;
}
.c-city-weather-current .current-weather__bg {
  padding: 24rpx 30rpx;
  background-size: 540rpx auto;
  background-repeat: no-repeat;
  background-position: right top;
}
.c-city-weather-current .current-abstract {
  margin: 26rpx auto 30rpx;
  max-width: 514rpx;
  min-height: 32rpx;
  font-size: 15rpx;
  line-height: 1.5;
  text-align: center;
  color: #000000;
}
.c-city-weather-current .current-basic {
  height: 84px;
  padding: 0 24px;
  border-radius: 12px;
  background-color: rgba(0, 0, 0, 0.03);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  margin-top: 20rpx;
}
.c-city-weather-current .current-basic .current-basic___item .value {
  font-size: 20rpx;
  font-weight: 500;
  color: #000000;
  white-space: nowrap;
}
.c-city-weather-current .current-basic .current-basic___item .desc {
  margin-top: 7rpx;
  font-size: 15rpx;
  font-weight: normal;
  color: rgba(0, 0, 0, 0.5);
}
.c-city-weather-current .current-weather__bg.index {
  padding: 20px;
  background-size: 346px auto;
}
.c-city-weather-current .air-tag:hover {
  color: white;
}
.c-city-weather-current .air-tag--1 {
  color: white;
  background-color: #95B359;
}
.c-city-weather-current .air-tag--2 {
  color: white;
  background-color: #A9A538;
}
.c-city-weather-current .air-tag--3 {
  color: white;
  background-color: #E0991D;
}
.c-city-weather-current .air-tag--4 {
  color: white;
  background-color: #D96161;
}
.c-city-weather-current .air-tag--5 {
  color: white;
  background-color: #A257D0;
}
.c-city-weather-current .air-tag--6 {
  color: white;
  background-color: #D94371;
}
.city-weather-sun {
  background-image: linear-gradient(225deg, #fee5ca, #e9f0ff 55%, #dce3fb);
}
.city-weather-sun .current-weather__bg {
  background-image: none;
}
.city-weather-cloudy {
  background-image: linear-gradient(225deg, #D0DAE8, #CCD4DF 55%, #A5B3C5);
}
.city-weather-cloudy .current-weather__bg {
  background-image: url(data:image/png;base64,);
}
.city-weather-rain {
  background-image: linear-gradient(225deg, #DBEBFF, #DDE5F4 55%, #BECDDD);
}
.city-weather-rain .current-weather__bg {
  background-image: url(data:image/png;base64,);
}
.city-weather-snow {
  background-image: linear-gradient(225deg, #D2F0FD, #E6F8FE 55%, #D6EDED);
}
.city-weather-snow .current-weather__bg {
  background-image: url(data:image/png;base64,);
}
.city-weather-haze {
  background-image: linear-gradient(225deg, #b29c82, #ded9d7 64.29%, #d9d7dc);
}
.city-weather-haze .current-weather__bg {
  background-image: url(data:image/png;base64,);
}

